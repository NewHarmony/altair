{
    "$schema": "https://vega.github.io/schema/vega-lite/v2.json",
    "data": {
        "url": "https://vega.github.io/vega-lite/data/stocks.csv"
    },
    "layer": [
        {
            "encoding": {
                "color": {
                    "field": "symbol",
                    "type": "nominal"
                },
                "x": {
                    "field": "date",
                    "type": "temporal"
                },
                "y": {
                    "field": "price",
                    "type": "quantitative"
                }
            },
            "mark": "line"
        }
    ]
}